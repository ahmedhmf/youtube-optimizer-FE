{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/youtube-optimizer-fe/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build:prod",
  "devCommand": "npm run start",
  "installCommand": "npm ci",
  "framework": null,
  "outputDirectory": "dist/youtube-optimizer-fe/browser"
}
